        <Label minHeight="25.0" style="-fx-font-weight: bold;" text="Delete User" />
        <Label text="User Name" GridPane.rowIndex="1" />
        <Label text="Password" GridPane.rowIndex="2" />
            <PasswordField fx:id="textPassword" GridPane.columnIndex="1" GridPane.rowIndex="2" />
