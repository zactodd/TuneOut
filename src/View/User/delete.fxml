<AnchorPane fx:id="popUpAnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UserNamePasswordRelatedController">
  <children>
    <GridPane alignment="TOP_CENTER" hgap="10.0" prefWidth="330.0" vgap="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="0.0">
